<Files ~ "^\.(htaccess|htpasswd)$">
deny from all
</Files>
AuthUserFile /etc/httpd/.htpasswd
AuthGroupFile /dev/null
AuthName "Develop WebPage"
AuthType Basic
require valid-user
order deny,allow


AddHandler application/x-httpd-php .php .html
php_flag allow_url_fopen on
php_flag allow_url_include on

